---
import Layout from "../page-builder/Layout.astro";
import PageBuilder from "../page-builder/PageBuilder.astro";
import { getPage } from "../api/directus";
import CustomWidget, {
	overrideWidgetList,
} from "../page-builder/CustomWidgets.astro";

const page = await getPage(import.meta.env.PUBLIC_ORGANIZATION_ID, "index");
---

<Layout>
	<PageBuilder
		page={page}
		CustomWidgets={CustomWidget}
		overrideWidgetList={overrideWidgetList}
	/>
</Layout>
